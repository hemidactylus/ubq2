[Unit]
Description=UBQ Trigger Checkbeat
After=syslog.target

[Service]
Type=simple
User=<<USERNAME>>
Group=<<GROUPNAME>>
StandardOutput=kmsg
StandardError=kmsg
WorkingDirectory=<<INSTALLATION_DIR>>/scripts
ExecStart=<<INSTALLATION_DIR>>/scripts/trigger_checkbeat.sh

[Install]
WantedBy=multi-user.target
